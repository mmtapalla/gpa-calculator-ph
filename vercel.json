{
    "version": 2,
    "builds": [
        {
            "src": "gpaCalculatorPh/manage.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "gpaCalculatorPh/manage.py"
        }
    ]
}
